{
  "name": "m3u8stream",
  "description": "Reads segments from a m3u8 or dash playlist into a consumable stream.",
  "keywords": [
    "m3u8",
    "hls",
    "dash",
    "live",
    "playlist",
    "segments",
    "stream"
  ],
  "version": "0.6.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/fent/node-m3u8stream.git"
  },
  "author": "fent (https://github.com/fent)",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha test/*-test.js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/*-test.js"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "dependencies": {
    "miniget": "^1.4.0",
    "sax": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/sax": "^1.0.1",
    "lolex": "^4.0.1",
    "mocha": "^6.0.0",
    "nock": "^10.0.0",
    "nyc": "^14.1.1",
    "typescript": "^3.4.2"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "MIT"
}
